{
	"name": "mui-daterange-picker",
	"version": "0.0.1",
	"private": true,
	"main": "dist/index.js",
	"files": [
		"dist/index.js",
		"dist/datepicker"
	],
	"homepage": ".",
	"dependencies": {
		"@babel/cli": "7.17.10",
		"@babel/core": "7.18.0",
		"@emotion/react": "11.9.0",
		"@emotion/styled": "11.8.1",
		"@mui/icons-material": "5.6.2",
		"@mui/material": "5.8.0",
		"@mui/styles": "^5.11.2",
		"classnames": "^2.3.2",
		"date-fns": "1.30.1",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-scripts": "5.0.1",
		"storybook": "6.5.0",
		"web-vitals": "2.1.4"
	},
	"scripts": {
		"start": "npm run storybook",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public",
		"delete": "if exist dist rmdir dist /s /q",
		"compile": "set NODE_ENV=production&&babel src --out-dir dist --copy-files"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@storybook/addon-actions": "6.5.0",
		"@storybook/addon-essentials": "6.5.0",
		"@storybook/addon-interactions": "6.5.0",
		"@storybook/addon-links": "6.5.0",
		"@storybook/builder-webpack5": "6.5.0",
		"@storybook/manager-webpack5": "6.5.0",
		"@storybook/node-logger": "6.5.0",
		"@storybook/preset-create-react-app": "4.1.0",
		"@storybook/react": "6.5.0",
		"@storybook/testing-library": "0.0.11",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.2.0",
		"@testing-library/user-event": "13.5.0",
		"babel-plugin-named-exports-order": "0.0.2",
		"prop-types": "15.8.1",
		"webpack": "5.72.1"
	}
}
